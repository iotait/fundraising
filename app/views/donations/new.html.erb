<%= form_for @donation do |form| %>
    <%= form.label :student_id %>
    <%= form.text_field :student_id %>
    <%= form.label :donor %>
    <%= form.text_field :donor %>
    <%= form.label :amount %>
    <%= form.text_field :amount %>
    <%= form.label :message %>
    <%= form.text_field :message %>
    <%= form.submit "Submit" %>
<% end %>