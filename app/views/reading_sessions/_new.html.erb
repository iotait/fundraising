<div id="new_reading_session_modal" class="modal fade" tabindex="-1">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">New Reading Session</h5>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>
      <div class="modal-body">
        <form>
          <div>
            <label class="mr-4" for="time">Time:</label>
            <input type="number" id="time" name="time" class="mb-4"></input>
          </div>
        </form>
      </div>
      <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-primary" data-dismiss="modal" id="add_reading_session_button">Add Reading Session</button>
      </div>
    </div>
  </div>
</div>