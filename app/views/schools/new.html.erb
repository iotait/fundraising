<%= form_for @school do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :email%>
    <%= form.text_field :email%>
    <%= form.label :phone %>
    <%= form.text_field :phone %>
    <%= form.label :address %>
    <%= form.text_field :address %>
    <%= form.label :about %>
    <%= form.text_field :about %>
    <%= form.label :goal %>
    <%= form.text_field :goal %>
    <%= form.submit "Submit" %>
<% end %>