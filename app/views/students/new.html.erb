<%= form_for @student do |form| %>
    <%= form.label :id%>
    <%= form.text_field :id%>
    <%= form.label :school_id%>
    <%= form.text_field :school_id%>
    <%= form.label :teacher_id%>
    <%= form.text_field :teacher_id%>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :goal %>
    <%= form.text_field :goal %>
    <%= form.submit "Submit" %>
<% end %>