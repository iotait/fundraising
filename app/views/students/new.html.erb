<%= form_for @student do |form| %>
    <%= form.label :first_name %>
    <%= form.text_field :first_name %>
    <%= form.label :last_name %>
    <%= form.text_field :last_name %>
    <%= form.label :email %>
    <%= form.text_field :email %>
    <%= form.label :about %>
    <%= form.text_field :about %>
    <%= form.label :goal %>
    <%= form.text_field :goal %>
    <%= form.submit "Submit" %>
<% end %>