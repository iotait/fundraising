<%= form_for @teacher do |form| %>
    <%= form.label :first_name %>
    <%= form.text_field :first_name %>
    <%= form.label :last_name %>
    <%= form.text_field :last_name %>
    <%= form.label :email%>
    <%= form.text_field :email%>
    <%= form.label :phone %>
    <%= form.text_field :phone %>
    <%= form.label :goal %>
    <%= form.text_field :goal %>
    <%= form.submit "Submit" %>
<% end %>